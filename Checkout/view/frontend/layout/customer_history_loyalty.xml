<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
         <referenceBlock name="page.main.title">
            <block class="Magento\Framework\View\Element\Template" name="title" template="Albatool_Checkout::title.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Albatool\Checkout\Block\Loyalty" name="form_history_loyalty_customer" template="Albatool_Checkout::history_loyalty.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>